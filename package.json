  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@rollup/plugin-typescript": "^8.3.1",
    "@testing-library/react": "^12.1.4",
    "@types/jest": "^27.4.1",
    "@types/react": "^17.0.39",
    "@typescript-eslint/eslint-plugin": "^5.14.0",
    "@typescript-eslint/parser": "^5.14.0",
    "babel-jest": "^27.5.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.29.3",
    "jest": "^27.5.1",
    "prettier": "^2.5.1",
    "rollup": "^2.70.0",
    "rollup-plugin-dts": "^4.2.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "typescript": "^4.6.2"
  },
  "peerDependencies": {
    "react": "^17.0.2"
  },
  "files": [
    "dist"
  ],
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "!**/index.ts"
    ],
    "json": true,
    "outputFile": "./coverage/coverage.json",
    "testEnvironment": "jsdom"
  },
  "keywords": [
    "react",
    "tabs"
  ],
  "license": "MIT",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "name": "@gavinsykes/react-tabbed-components",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/gavinsykes"
  },
  "scripts": {
    "build": "npm run rollup",
    "ci": "npm run build && npm run test:cover",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "lint:fix": "eslint 'src/**/*.{js,jsx,ts,tsx}' --fix",
    "postversion": "git push origin main && git push origin main --tags && npm publish",
    "preversion": "npm run build && npm run test:lint",
    "rollup": "rollup -c",
    "setup": "npm install",
    "test": "jest",
    "test:lint": "npm run lint:fix && npm run test"
